    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia de Perguntas</title>
</head>
<body>
    <h1>Guia de Perguntas</h1>
    <br>
    <h3>Lista de perguntas</h3>

    <p>Nome: <%=nome %> </p>
    <p>Linguagm favorita: <%=lang %> </p>
    <p>Empresa: <%=empresa %> </p>
    <p>Inscritos: <%=inscritos %> </p>

    <% if (msg === true) { %>
        <h3>Isso Ã© uma mensagem de erro!</h3>
    <% } else { %> 
        <h3>Nenhum erro!</h3>
    <% } %>

    <% produtos.forEach(function(produtos) { %>
        <hr>
        <h4><%=produtos.nome %> </h4> 
        <h5><%=produtos.preco  %> R$</h5>
    <%}) %>

    
</body>
</html>